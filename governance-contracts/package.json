{
    "name": "panvala-governance",
    "version": "0.0.1",
    "description": "Governance contracts for Panvala",
    "main": "truffle-config.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "compile": "yarn sol-lint && truffle compile",
        "coverage": "./node_modules/.bin/solidity-coverage",
        "ganache-cli": "../tools/run_ganache_cli.sh",
        "lint": "./node_modules/.bin/eslint",
        "migrate": "./node_modules/.bin/truffle migrate",
        "migrate:lptc": "./node_modules/.bin/truffle exec scripts/launch_partners_token_capacitor.js",
        "sol-lint": "./node_modules/.bin/solhint ./contracts/**/*.sol",
        "fix": "./node_modules/.bin/eslint --fix ./",
        "test": "yarn lint ./ && truffle test"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "eslint": "5.16.0",
        "eslint-config-airbnb-base": "13.1.0",
        "eslint-plugin-import": "2.17.3",
        "ganache-cli": "^6.6.0",
        "solhint": "2.1.0",
        "solidity-coverage": "^0.6.0-beta.5",
        "truffle": "5.0.31"
    },
    "dependencies": {
        "bs58": "4.0.1",
        "ethereumjs-util": "6.1.0",
        "ethers": "^4.0.33",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "openzeppelin-solidity": "2.1.3",
        "prettier": "^1.18.2",
        "truffle-hdwallet-provider": "1.0.16"
    }
}
